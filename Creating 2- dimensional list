x = [[ 4, 5, 6, 11],
    [ 77, -4, 6, 2],
    [ 1, 22, 8, 0]]

#print out the x 2-demensional list
print ("x = : ", x)
#print out the 2 row = 6
print ("x = : ", x[1][2])
#print out the 1st row -1 = 111
print ("x = : ", x[0][-1])

#declaring the enmpty list
columns = [];

#for loop print out the 3 colums = 6,,68
for rows in x:
  columns.append(rows[2])

  print("The third columns is: ", columns)
