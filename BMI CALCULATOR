#Here are the if statement for python!
a = 1
b = 2
if a < b :
    print("a is less than b")
    print("a is definitly less than b")
print("not sure if b is less than a")

c = 3
d = 4
if c < d:
    print("c is less than d")
else:
    print("c is NOT less than d")
    print("i dont think c is than d")
print("outside the if block")

e = 10
f = 8
if e < f:
    print("e is less than f")
elif e==f:
    print("e is equal to f")
else:
    print("e is greater than f")

e = 20
f = 8
if e < f:
    print("e is less than f")
elif e==f:
    print("e is equal to f")
elif e > f + 10:
    print("e is greater than more than 10")
else:
    print("e is greater than f")


g = 8
h = 8
if 7 < 8:
    print("g is less than h")
else:
    if g==h:
        print("g is equal to h")
    else:
        print("g is greater than h")

name = "nerio"
height_m = 2
weight_kg = 110

bmi = weight_kg / (height_m ** height_m)
print("bmi: ")
print("bmi")
if bmi > 25:
    print(name)
    print("is not overwight")
else:
    print(name)
    print("is overweight")
